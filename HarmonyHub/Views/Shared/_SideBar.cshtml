<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link rel="stylesheet" href="~/css/sidebar.css" />
	<link rel="stylesheet" href="~/css/player.css" />
	<title>Landing</title>
</head>
<body>
	
	<partial name="_SideBarPartial.cshtml" />
	<div class="topbar">
		<div class="prev-next-buttons">
			@* 		<button type="button" class="fa fas fa-chevron-left"></button>
			<button type="button" class="fa fas fa-chevron-right"></button> *@
		</div>

		<div class="navbar">
			<a asp-action="Index" asp-controller="Profile">
				<button style="background-color: rgb(241, 126, 32);">PROFILE</button>
			</a>
			<form id="logoutForm" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
				<button id="logout" type="submit">LOG OUT</button>
			</form>

		</div>
	</div>
	<div role="main" >
		@RenderBody()
	</div>

	<partial name="_Player.cshtml" />



	<script src="~/js/fa.js" crossorigin="anonymous"></script>
</body>

<script src="~/js/player.js"></script>
<script>
	function search_submit() {
		document.getElementById("search").submit();
	}
</script>

</html>
